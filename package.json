    "wdio": "wdio run ./wdio.conf.js"
  },
  "keywords": ["automation", "testing", "webdriverio", "cucumber", "saucedemo"],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "devDependencies": {
    "@wdio/cli": "^9.16.2",
    "@wdio/cucumber-framework": "^9.16.2",
    "@wdio/local-runner": "^9.16.2",
    "@wdio/spec-reporter": "^9.16.2",
    "@wdio/visual-service": "^8.0.4",
    "wdio-wait-for": "^3.1.0"
  }
}
