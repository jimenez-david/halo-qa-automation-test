{
  "name": "halo-automation-exercise",
  "version": "1.0.0",
  "description": "Automated QA testing framework for SauceDemo using WebdriverIO and Cucumber",
  "main": "index.js",
  "scripts": {
    "test": "wdio run ./wdio.conf.js",
    "test:login": "wdio run ./wdio.conf.js --spec ./features/login.feature",
